<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Allure Remodeling and Home Improvement- @ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <script src='https://www.google.com/recaptcha/api.js'></script>
    <link href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css" rel=" stylesheet" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link href="~/Content/MainSite.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jquery-confirm")
    @Scripts.Render("~/bundles/glide")
    @RenderSection("styles", required: false)
    @RenderSection("scripts", required: false)

    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.1/css/all.css" integrity="sha384-gfdkjb5BdAXd+lj+gudLWI+BXq4IuLW5IT+brZEZsLFm++aCMlF1V92rMkPaX4PP" crossorigin="anonymous">
    <link href="https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css" rel="stylesheet">
    <link href="https://cdn.datatables.net/buttons/1.5.2/css/buttons.dataTables.min.css" rel="stylesheet" />
    <script src="https://gitcdn.github.io/bootstrap-toggle/2.2.2/js/bootstrap-toggle.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.13/js/jquery.dataTables.js"></script>
    <script src="https://cdn.datatables.net/select/1.2.1/js/dataTables.select.min.js" type="text/javascript"></script>
    <script src=" https://cdn.datatables.net/buttons/1.5.2/js/dataTables.buttons.min.js" type="text/javascript"></script>
    <script src="https://cdn.datatables.net/buttons/1.5.2/js/buttons.flash.min.js" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.36/pdfmake.min.js" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.36/vfs_fonts.js" type="text/javascript"></script>
    <script src="https://cdn.datatables.net/buttons/1.5.2/js/buttons.html5.min.js" type="text/javascript"></script>
    <script src="https://cdn.datatables.net/buttons/1.5.2/js/buttons.print.min.js" type="text/javascript"></script>
</head>
<body>
    <div class="container-fullwidth page-header">
        <div class="row">
            <div class="glyphicon glyphicon-grain form-inline center-block"><h4 class="text-muted"><u>Turn your House into a Beautiful Home</u></h4><div class="glyphicon glyphicon-grain form-inline"></div></div>
            <div class="container-fluid col-md-12">
                ​<picture>
                    <img src="~/Content/allureforblue.jpg" class="img-fluid img-thumbnail mx-auto d-block border-0" alt="allure logo">
                </picture>
                <div class="container-fluid col-md-12 bg-light text-dark border border-dark">
                    <h3>Repairs Kitchens Bathrooms Sunrooms Faucets&Sinks Tubs&Showers Entry Doors Flooring Cabinets Countertops &More </h3>
                </div>
                <nav class="navbar-expand-lg border border-dark ">
                    <form>
                        <a href="#" class="btn btn-lg btn-light text-primary border border-primary"><u>Home</u></a>
                        <a href="#" class="btn btn-lg btn-light text-primary border border-primary"><u>About Us</u></a>
                        <a href="#" class="btn btn-lg btn-light text-primary border border-primary"><u>Services</u></a>
                        <a href="#" class="btn btn-lg btn-light text-primary border border-primary"><u>Gallery</u></a>
                        <a href="#" class="btn btn-lg btn-light text-primary border border-primary"><u>Customer Reviews</u></a>
                        <a href="#" class="btn btn-lg btn-light text-primary border border-primary"><u>Careers</u></a>
                        <a href="#" class="btn btn-lg btn-light text-primary border border-primary"><u>Request an Estimate</u></a>
                        <a href="@Url.Action("CreateCustomerProfile","Home")" class="btn btn-lg btn-light text-primary border border-primary"><u>Create a Profile</u></a>
                        <a id="loginButton" href="@Url.Action("Login", "Home")" class="btn btn-lg btn-light text-primary border border-primary"><u>Login</u></a>
                        <a id="logoutButton" href="@Url.Action("LogOut", "Home")" class="btn btn-lg btn-light text-primary border border-primary"><u>Login</u></a>
                        @if(Session["LoggedIn"] != null)
                        {
                        <a id="\" href="@Url.Action("ViewProfile", "Home")" class="btn btn-lg btn-light text-primary border border-primary"><u>View Profile</u></a>
                        }
                        

                        
                    </form>
                </nav>
            </div>
        </div>
    </div>
    <div class="main-content container-fluid">
        <div class="row">
            <div class="container-fluid col-md-12  border border-dark">
                @RenderBody()
            </div>
        </div>
    </div>
    <div class="footer container-fluid col-md-12 border border-dark">
        <hr />
        <footer>
            <p>Ready to meet your home improvement needs?</p>
            <p>Ready to trust fully insured, highly reputable, and recommended general contractors?</p>
            <p><a href="#" class="btn btn-md btn-light text-primary border border-primary"><u>Request an Estimate Today!</u></a></p>
            <p><div class="glyphicon glyphicon-grain form-inline"><h4>Remodeling. Repairs. Home Improvements</h4><div class="glyphicon glyphicon-grain form-inline"></div></div></p>
            <p><div class="glyphicon glyphicon-earphone form-inline center-block">Questions? Call(513)237-8100</div></p>
            <p>&copy; @DateTime.Now.Year - Allure Remodeling and Home Improvement</p>
        </footer>
    </div>
  
    <div class="loader"></div>

    <script>

        $(function ()
        {
            console.log("Test");
            $.ajax
            ({
                type: 'POST',
                url: '@Url.Action("GetLoginStatus", "Home")',
                dataType: 'json',
                contentType: "application/json; charset=utf-8",
                success: function (data)
                {
                    // Is user logged in?
                    if (data == true)
                    {
                        // Yes
                        $("#logoutButton").show();
                        $("#accountProfileButton").show();
                        $("#loginButton").hide();
                    }
                    else 
                    {
                        // No
                        $("#logoutButton").hide();
                        $("#accountProfileButton").hide();
                    }
                }
            });
        })
        
        function showErrorMessage(Title, Message)
        {
            $.confirm
                ({
                    title: Title,
                    content: Message,
                    type: 'red',
                    typeAnimated: true,
                    buttons:
                    {
                        tryAgain:
                        {
                            text: 'OK',
                            btnClass: 'btn-red',
                            action: function () {

                            }
                        }
                    }
                });
        }

        function showInfoMessage(Title, Message)
        {
            $.confirm
                ({
                    title: Title,
                    content: Message,
                    type: 'blue',
                    typeAnimated: true,
                    buttons:
                    {
                        tryAgain:
                        {
                            text: 'OK',
                            btnClass: 'btn-blue',
                            action: function () {

                            }
                        }
                    }
                });
        }

        function showSuccessMessage(Title, Message) {
            $.confirm
                ({
                    title: Title,
                    content: Message,
                    type: 'green',
                    typeAnimated: true,
                    autoClose: 'OK|3000',
                    buttons:
                    {
                        OK:
                        {
                            text: 'OK',
                            btnClass: 'btn-green',
                            action: function () {

                            }
                        }
                    }
                });
        }

        function getUrlParameter(name) {
            name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
            var regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
            var results = regex.exec(location.search);
            return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
        }


    </script>
</body>
</html>
